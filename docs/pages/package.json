{
  "name": "swit-docs",
  "version": "1.0.0",
  "description": "Documentation website for Swit Go Microservice Framework",
  "scripts": {
    "dev": "vitepress dev",
    "build": "vitepress build",
    "preview": "vitepress preview",
    "generate-api-docs": "node scripts/api-docs-generator.js",
    "sync-docs": "node scripts/sync-docs.js",
    "build-all": "./scripts/build-all.sh",
    "build-optimized": "npm run build && npm run optimize",
    "optimize": "node scripts/performance-optimizer.js",
    "validate-content": "node scripts/content-validator.js",
    "clean": "rm -rf dist .vitepress/cache",
    "clean-all": "npm run clean && rm -rf node_modules package-lock.json",
    "lint": "eslint . --ext .vue,.js,.ts --fix",
    "lint:check": "eslint . --ext .vue,.js,.ts",
    "test": "vitest",
    "test:unit": "vitest run --reporter=verbose",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:accessibility": "node tests/accessibility/run-a11y-tests.js",
    "test:performance": "node tests/performance/run-perf-tests.js",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:accessibility",
    "security:scan": "node scripts/security-scanner.js",
    "security:audit": "npm audit && npm run security:scan",
    "analyze": "npm run build && npm run optimize"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vue/test-utils": "^2.4.0",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.8.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "happy-dom": "^18.0.1",
    "lighthouse": "^11.4.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vitepress": "^1.0.0-rc.31",
    "vitest": "^1.0.0",
    "vue": "^3.3.10"
  },
  "dependencies": {
    "@sentry/browser": "^7.91.0",
    "@vueuse/core": "^10.7.0",
    "shiki": "^0.14.7"
  }
}