# Docker Compose 环境变量
# 这些变量将被传递给 Docker 构建过程

# 构建版本信息
VERSION=dev
BUILD_TIME=2025-07-11T00:00:00Z
GIT_COMMIT=unknown

# 数据库配置
MYSQL_ROOT_PASSWORD=root
MYSQL_CHARACTER_SET_SERVER=utf8mb4
MYSQL_COLLATION_SERVER=utf8mb4_unicode_ci

# 服务配置
AUTH_SERVICE_PORT=9001
MAIN_SERVICE_PORT=9000
CONSUL_PORT=8500
MYSQL_PORT=3306

# 网络配置
SUBNET=172.20.0.0/16
